{
    "name": "do",
    "displayName": "Do",
    "description": "quick and dirty automation",
    "version": "0.0.2",
    "repository": "https://github.com/dasdeck/do",
    "license": "MIT",
    "publisher": "jmanuels",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "do",
                "title": "do"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "publish":"vsce publish && git add . && git commit -m=$M && git push"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "eslint": "^4.9.0",
        "lodash": "^4.17.4",
        "sinon": "^4.0.1",
        "tslint": "^5.7.0",
        "typescript": "^2.5.3",
        "vscode": "^1.1.5"
    },
    "dependencies": {
        "async": "^2.5.0"
    }
}
